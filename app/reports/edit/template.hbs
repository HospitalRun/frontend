{{#edit-panel editPanelProps=editPanelProps}}
  {{#em-form model=model submitButton=false }}
    {{#if model.visit}}
      {{patient-summary
        patient=model.visit.patient
        returnTo='visit.edit'
        returnToContext=model.id
        disablePatientLink=model.isNew
      }}
    {{/if}}
  {{/em-form}}
{{/edit-panel}}