{{#modal-dialog
    isUpdateDisabled=isUpdateDisabled
    title=(t (if model.isNew 'admin.lookup.edit.titles.add' 'admin.lookup.edit.titles.edit'))
    updateButtonAction=updateButtonAction
    updateButtonText=updateButtonText}}
  {{#em-form model=model submitButton=false action="update" }}
    {{#if model.isLookupTypeNameValue}}
      {{em-input label=(t 'admin.lookup.edit.labels.name') property="name" class="lookup-type-name"}}
    {{/if}}
    {{em-input label=(t 'admin.lookup.edit.labels.value') property="value" class="lookup-type-value"}}
  {{/em-form}}
{{/modal-dialog}}
