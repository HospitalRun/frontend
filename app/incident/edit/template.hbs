{{#em-form model=this submit_button=false }}
        <div class="panel panel-success">
            <div class="panel-body">
                <div class="row">        
                    <div class="form-group col-sm-2">
                        <label class="control-label">
                            Incident Id
                        </label>
                        {{id}}
                    </div>                    
                </div>
            </div>
        </div>
        <ul class="nav nav-pills" role="tablist">
            <li role="presentation" class="active" ><a href="#creation" role="tab" data-toggle="pill">Creation</a></li>
            <li role="presentation"><a href="#feedback" role="tab" data-toggle="pill">Feedback</a></li>    
            <li role="presentation"><a href="#investigate" role="tab" data-toggle="pill">Investigation/Factors</a></li>
            <li role="presentation"><a href="#risk" role="tab" data-toggle="pill">Risk Assesment</a></li>
            <li role="presentation"><a href="#recommendation" role="tab" data-toggle="pill">Recommendations/Summary</a></li>
        </ul>
    <div class="tab-content">
        <div role="tabpanel" class="tab-pane active" id="creation">        
            <div class="panel detail-section">
                <div class="panel-heading">
                    <h3 class="panel-title">
                        General Information
                    </h3>
                </div>
                <div class="panel-body detail-section-content">
                    <div class="row">
                        <div class="col-xs-12 col-sm-6">
                            <div class="form-group has-success">
                                <label class="control-label">Reported By</label>
                                <div class="form-control not-editable">
                                    {{#if isNew}}
                                        {{setAndGetReportedBy}}
                                    {{else}}
                                        {{reportedBy}}
                                    {{/if}}
                                </div>
                            </div>
                        </div>
                        <div class="col-xs-12 col-sm-6">
                            {{em-input property="locationOfIncident" label="Location of Incident" class="required"}}
                        </div>
                     </div>
                     <div class="row">
                        {{date-picker property="dateOfIncident" label="Date of Incident" class="col-xs-12 col-sm-6"}}
                        {{em-input property="timeOfIncident" label="Time of Incident" class="col-xs-12 col-sm-6"}}
                      </div>                        
                    {{em-text label="Incident Description" property="incidentDescription" rows=3}}
                </div>
            </div>
            <div class="panel detail-section">
                <div class="panel-heading">
                    <h3 class="panel-title">
                            Incident Type
                    </h3>
                </div>
                <div class="panel-body detail-section-content">
                    <div class="row">
                        {{em-input property="incidentMedicalCategoryName" label="Category Name" class="col-xs-12 col-sm-6"}}
                        {{em-input property="incidentMedicalCategoryItem" label="Category Item" class="col-xs-12 col-sm-6"}}
                        {{em-text  property="incidentMedicalCategoryOther" label="Other" rows=3 class="col-xs-12 col-sm-6"}}
                        <!--{{em-select
                            prompt=" "
                            label="Category Name"
                            property="incidentMedicalCategoryName"
                            content=incidentCategoryNameList
                            optionValuePath="content"
                            optionLabelPath="content"
                            class="col-xs-12 col-sm-6"
                        }}
                        {{select-or-typeahead property="name" label="Category Name" list=incidentCategoryNameList selection=name }}-->
                    </div>
                </div>
             </div>
        </div>
        <div role="tabpanel" class="tab-pane" id="feedback">
                <div class="panel">
                    <div class="panel-heading">
                        <h3 class="panel-title">
                            Feedbacks
                            {{#if canAddFeedback}}
                                <button class="btn btn-primary align-right" {{action "showAddFeedback" bubbles=false }}><span class="octicon octicon-plus"></span>New Feedback</button>
                            {{/if}}
                        </h3>
                    </div>
                    <div class="panel-body detail-section-content">
                        <table class="table">
                            <tr class="table-header">
                                <th>Date</th>
                                <th>Given By</th>
                                <th>Feedback</th>
                                <th>Actions</th>
                            </tr>
                            {{#with this as parentController}}
                                {{#each feedbacks}}
                                        <td>{{date-format dateRecorded}}</td>
                                        <td>{{givenBy}}</td>
                                        <td>{{description}}</td>
                                        <td>
                                            {{#if parentController.canAddFeedback}}
                                               <button class="btn btn-default neutral" {{action "showEditFeedback" this}}>Edit</button>
                                             {{/if}}
                                             {{#if parentController.canDeleteFeedback}}
                                                <button class="btn btn-default warning" {{action "showDeleteFeedback" this  bubbles=false }}><span class="octicon octicon-x"></span>Delete</button>
                                             {{/if}}
                                        </td>
                                    </tr>
                                {{/each}}
                            {{/with}}
                        </table>
                    </div>
                </div>
        </div>
