                                                            </tr>
                                                        {{/each}}
                                                    </TBODY>
                                                </table>
                                            </td>
                                        </tr>
                                    {{/each}}
                            </TBODY>
                        </table>
                        <table class="table">
                            <THEAD>
                                <tr class="table-header success">
                                    <th>Communication Factors</th>
                                    <th>Components</th>
                                </tr>
                            </THEAD>
                            <TBODY>
                                    {{#each model.communicationFactors as |communicationFactor|}}
                                        <tr>
                                            <td>
                                                {{communicationFactor.factorType}}
                                            </td>
                                            <td>
                                                <table class="table">
                                                    <TBODY>
                                                        {{#each communicationFactor.components as |component|}}
                                                            <tr>
                                                                {{em-checkbox label=component.name property=component.id}}
                                                            </tr>
                                                        {{/each}}
                                                    </TBODY>
                                                </table>
                                            </td>
                                        </tr>
                                    {{/each}}
                            </TBODY>
                        </table>
                        <table class="table">
                            <THEAD>
                                <tr class="table-header success">
                                    <th>Equipment Factors</th>
                                    <th>Components</th>
                                </tr>
                            </THEAD>
                            <TBODY>
                                    {{#each model.equipmentFactors as |equipmentFactor|}}
                                        <tr>
                                            <td>
                                                {{equipmentFactor.factorType}}
                                            </td>
                                            <td>
                                                <table class="table">
                                                    <TBODY>
                                                        {{#each equipmentFactor.components as |component|}}
                                                            <tr>
                                                                {{em-checkbox label=component.name property=component.id}}
                                                            </tr>
                                                        {{/each}}
                                                    </TBODY>
                                                </table>
                                            </td>
                                        </tr>
                                   {{/each}}
                            </TBODY>
                        </table>
                        <table class="table">
                            <THEAD>
                                <tr class="table-header success">
                                    <th>Work Environment Factors</th>
                                    <th>Components</th>
                                </tr>
                            </THEAD>
                            <TBODY>
                                    {{#each model.workEnvFactors as |workEnvFactor|}}
                                        <tr>
                                            <td>
                                                {{workEnvFactor.factorType}}
                                            </td>
                                            <td>
                                                <table class="table">
                                                    <TBODY>
                                                        {{#each workEnvFactor.components as |component|}}
                                                            <tr>
                                                               {{em-checkbox label=component.name property=component.id}}
                                                            </tr>
                                                        {{/each}}
                                                    </TBODY>
                                                </table>
                                            </td>
                                        </tr>
                                    {{/each}}
                            </TBODY>
                        </table>
                        <table class="table">
                            <THEAD>
                                <tr class="table-header success">
                                    <th>Organisational Factors</th>
                                    <th>Components</th>
                                </tr>
                            </THEAD>
                            <TBODY>
                                    {{#each model.organisationalFactors as |organisationalFactor|}}
                                        <tr>
                                            <td>
                                                {{organisationalFactor.factorType}}
                                            </td>
                                            <td>
                                                <table class="table">
                                                    <TBODY>
                                                        {{#each organisationalFactor.components as |component|}}
                                                            <tr>
                                                                {{em-checkbox label=component.name property=component.id}}
                                                            </tr>
                                                        {{/each}}
                                                    </TBODY>
                                                </table>
                                            </td>
                                        </tr>
                                    {{/each}}
                            </TBODY>
                        </table>
                        <table class="table">
                            <THEAD>
                                <tr class="table-header success">
                                    <th>Education and Training Factors</th>
                                    <th>Components</th>
                                </tr>
                            </THEAD>
                            <TBODY>
                                    {{#each model.educationAndTrainingFactors as |educationAndTrainingFactor|}}
                                        <tr>
                                            <td>
                                                {{educationAndTrainingFactor.factorType}}
                                            </td>
                                            <td>
                                                <table class="table">
                                                    <TBODY>
                                                        {{#each educationAndTrainingFactor.components as  |component|}}
                                                            <tr>
                                                                {{em-checkbox label=component.name property=component.id}}
                                                            </tr>
                                                       {{/each}}
                                                    </TBODY>
                                                </table>
                                            </td>
                                        </tr>
                                    {{/each}}
                            </TBODY>
