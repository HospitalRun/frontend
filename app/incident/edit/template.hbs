                                        <td>
                                            {{patientFactor.factorType}}
                                        </td>
                                        <td>
                                            <table class="table">
                                                <TBODY>
                                                    {{#each patientFactor.components as |component|}}
                                                        <tr>
                                                            {{em-checkbox label=component.name property=component.id}}
                                                         </tr>
                                                    {{/each}}
                                                </TBODY>
                                            </table>
                                        </td>
                                    </tr>
                                {{/each}}
                            </TBODY>
                        </table>
                        <table class="table">
                                <THEAD>
                                    <tr class="table-header success">
                                        <th>Staff Factors</th>
                                        <th>Components</th>
                                    </tr>
                                </THEAD>
                                <TBODY>
                                    {{#each model.staffFactors as |staffFactor|}}
                                        <tr>
                                            <td>
                                                {{staffFactor.factorType}}
                                            </td>
                                            <td>
                                                <table class="table">
                                                    <TBODY>
                                                        {{#each staffFactor.components as |component|}}
                                                            <tr>
                                                               {{em-checkbox label=component.name property=component.id}}
                                                             </tr>
                                                        {{/each}}
                                                    </TBODY>
                                                </table>
                                            </td>
                                        </tr>
                                   {{/each}}
                                </TBODY>
                        </table>
                        <table class="table">
                            <THEAD>
                                <tr class="table-header success">
                                    <th>Task Factors</th>
                                    <th>Components</th>
                                </tr>
                            </THEAD>
                            <TBODY>
                                    {{#each model.taskFactors as |taskFactor|}}
                                        <tr>
                                            <td>
                                                {{taskFactor.factorType}}
                                            </td>
                                            <td>
                                                <table class="table">
                                                    <TBODY>
