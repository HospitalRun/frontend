                            <THEAD>
                                <tr class="table-header success">
                                    <th>Team Factors Factors</th>
                                    <th>Components</th>
                                </tr>
                            </THEAD>
                            <TBODY>
                                {{#each model.teamFactors as |teamFactor|}}
                                    <tr>
                                        <td>
                                            {{teamFactor.factorType}}
                                        </td>
                                        <td>
                                            <table class="table">
                                                <TBODY>
                                                    {{#each teamFactor.components as |component|}}
                                                        <tr>
